{
  "name": "online-store-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "VITE_SIDE_MODE=SSR ts-node server.ts",
    "dev": "VITE_SIDE_MODE=CSR vite",
    "build": "VITE_SIDE_MODE=CSR tsc && vite build && cp _redirects ./dist/_redirects",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc --project tsconfig.json",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write"
  },
  "dependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@netlify/functions": "^1.0.0",
    "@react-oauth/google": "^0.11.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/express": "^4.17.21",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "express": "^4.18.2",
    "jwt-decode": "^4.0.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hydration-provider": "^2.1.0",
    "react-paginate": "^8.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-select": "^5.7.7",
    "react-slider": "^2.0.6",
    "redux-persist": "^6.0.0",
    "sass": "^1.66.1",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.1",
    "vite-plugin-ssr": "^0.4.13"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-slider": "^1.3.1",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.46.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.9"
  }
}
